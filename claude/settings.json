          }
        ]
      },
      {
        "matcher": "permission_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/q.yu/.claude/hooks/tmux-notify.sh permission_prompt"
          }
        ]
      }
    ]
